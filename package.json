    "eslint": "^7.25.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-tree-shaking": "^1.9.0",
    "husky": "^4.3.0",
    "prettier": "^2.2.1",
    "sass": "^1.34.0",
    "ts-dedent": "^2.1.1",
