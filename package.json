  "dependencies": {
    "@traptitech/markdown-it-katex": "^3.4.3",
    "@traptitech/markdown-it-regexp": "^0.5.3",
    "@traptitech/markdown-it-spoiler": "^1.1.6",
    "core-js": "^3.12.0",
    "highlight.js": "^11.0.0",
    "katex": "^0.13.9",
    "markdown-it": "^12.0.6",
    "markdown-it-container": "^3.0.0",
    "markdown-it-image-filter": "^1.1.0",
